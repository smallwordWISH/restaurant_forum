  <% if restaurant.is_favorited?(current_user) %>
    <%= link_to '',unfavorite_restaurant_path(restaurant), method: :post, class: "heart-btn glyphicon glyphicon-heart" %>
  <% else %>
    <%= link_to '',favorite_restaurant_path(restaurant), method: :post, class: "heart-btn-empty glyphicon glyphicon-heart-empty" %>
  <% end %>